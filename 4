#include <stdio.h>
#include <windows.h>

int main() {
    SetConsoleCP(1251);
    SetConsoleOutputCP(1251);
    int n, i, count = 0;
    printf("Введіть натуральне число n: ");
    scanf("%d", &n);
    for (i = 1; i <= n; i++) {
        if (n % i == 0) {
            count++;
        }
    }
    printf("Кількість рівних дільників числа %d: %d\n", n, count);
    return 0;
}
