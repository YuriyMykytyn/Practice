#include <stdio.h>
#include <windows.h>
#include <stdlib.h>


int gcd(int a, int b) {
    if (b == 0)
        return a;
    return gcd(b, a % b);
}

int lcm(int a, int b) {
    return (a * b) / gcd(a, b);
}

int main() {
        SetConsoleCP(1251);
    SetConsoleOutputCP(1251);
    int n, i, numbers[20], result;

    printf("Введіть кількість чисел (р): ");
    scanf("%d", &n);

    printf("Введіть %d натуральних чисел: ", n);
    for (i = 0; i < n; i++) {
        scanf("%d", &numbers[i]);
    }

    result = numbers[0];
    for (i = 1; i < n; i++) {
        result = lcm(result, numbers[i]);
    }

    printf("Найменше спільне кратне: %d\n", result);

    return 0;
}
